<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width,initial-scale=1.0">
        <title>Beyond Doubt Ministries</title>
        @RenderPage("~/_commonCSS.cshtml")
    </head>
    <body class="site-skin">
        @RenderPage("~/_header.cshtml")
        <div id="page" class="page page-skin">
             @RenderPage("~/_mainNav.cshtml")
            <div id="main-img-wrapper" class="main-img-wrapper">
                <img src="@Href("~/images/cross.png")" alt=""/>
            </div>
            <div id="content" class="content content-skin">
                <h2>@Page.Title</h2>
                @RenderBody()
            </div>
        </div>
        @RenderPage("~/_footer.cshtml")
        @RenderPage("~/_commonJS.cshtml")
    </body>
</html>
